  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:css": "npm-run-all build:css:sass build:css:bannerize",
    "build:css:bannerize": "bannerize dist/videojs-seek-buttons.css --banner=scripts/banner.ejs",
    "build:css:sass": "node-sass src/plugin.scss dist/videojs-seek-buttons.css --output-style=compressed --linefeed=lf",
    "build:js": "npm-run-all build:js:rollup-modules build:js:rollup-umd build:js:bannerize build:js:uglify",
    "build:js:babel": "babel src -d es5",
    "build:js:bannerize": "bannerize dist/videojs-seek-buttons.js --banner=scripts/banner.ejs",
    "build:js:browserify": "browserify . -s videojs-seek-buttons -o dist/videojs-seek-buttons.js",
    "build:js:rollup-modules": "rollup -c scripts/modules.rollup.config.js",
    "build:js:rollup-umd": "rollup -c scripts/umd.rollup.config.js",
    "build:js:uglify": "uglifyjs dist/videojs-seek-buttons.js --comments --mangle --compress  -o dist/videojs-seek-buttons.min.js",
    "build:lang": "vjslang --dir dist/lang",
    "build:test": "rollup -c scripts/test.rollup.config.js",
    "build:test:browserify": "browserify  `find test -name '*.test.js'` -t babelify -o dist-test/videojs-seek-buttons.js",
    "clean": "rimraf dist test/dist",
    "docs": "npm-run-all -p docs:*",
    "docs:api": "documentation src/*.js -f html -o docs/api",
    "docs:toc": "doctoc README.md",
    "lint": "vjsstandard",
    "mkdirs": "mkdir -p dist dist-test es5",
    "postclean": "mkdirp dist test/dist",
    "postversion": "git push origin master && git push origin --tags",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "prepush": "npm run lint",
    "prestart": "npm run build",
    "pretest": "npm-run-all lint build",
    "preversion": "npm test",
    "start": "npm-run-all -p start:server watch",
    "start:serve": "babel-node scripts/server.js",
    "start:server": "static -a 0.0.0.0 -p 9999 -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}' .",
    "test": "karma start test/karma.conf.js",
    "test:chrome": "npm run pretest && karma start test/karma/chrome.js",
    "test:firefox": "npm run pretest && karma start test/karma/firefox.js",
    "test:ie": "npm run pretest && karma start test/karma/ie.js",
    "test:safari": "npm run pretest && karma start test/karma/safari.js",
    "version": "node scripts/version.js",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "node-sass src/plugin.scss dist/videojs-seek-buttons.css --output-style=compressed --linefeed=lf --watch src/**/*.scss",
    "watch:js": "watchify src/plugin.js -t babelify -v -o dist/videojs-seek-buttons.js",
    "watch:js-modules": "rollup -c scripts/modules.rollup.config.js -w",
    "watch:js-umd": "rollup -c scripts/umd.rollup.config.js -w",
    "watch:test": "rollup -c scripts/test.rollup.config.js -w"
  },
